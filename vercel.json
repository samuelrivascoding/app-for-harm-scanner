{
  "version": 2,
  "builds": [
    {
      "src": "server/api/analyze.js",
      "use": "@vercel/node"
    }, 
    {
      "src": "server/api/virtualassistant.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },

    {
      "src": "/server/api/photoupload",
      "methods": ["POST"],
      "dest": "server/api/photoupload.cjs"
    },
    {
      "src": "/server/api/analyze",
      "methods": ["POST"],
      "dest": "server/api/analyze.js"
    },
    {
      "src": "/server/api/virtualassistant",
      "methods": ["POST"],
      "dest": "server/api/virtualassistant.js"
    }
  ]
}
